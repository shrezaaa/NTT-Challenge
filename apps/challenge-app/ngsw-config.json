{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": ["/favicon.ico", "/*.css", "/*.js"]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": ["/assets/**", "/*.(png|jpg|jpeg|svg)"]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "articles-api",
      "urls": [
        "https://api.realworld.show/api/articles",
        "https://api.realworld.show/api/articles/**"
      ],
      "cacheConfig": {
        "maxSize": 50,
        "maxAge": "1d",
        "timeout": "10s",
        "strategy": "freshness"
      }
    }
  ]
}
